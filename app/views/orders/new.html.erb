<h1>New Lunch Order</h1>
<!-- <%= link_to 'New Lunch Order', new_order_path %> -->
<%= form_for :order,url: orders_path do |f| %>

  <p>
    <%= f.label :date %><br>
    <%= f.text_field :date %>
  </p>
 
  <p>
    <%= f.label :order %><br>
    <%= f.text_area :order %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', orders_path %>

